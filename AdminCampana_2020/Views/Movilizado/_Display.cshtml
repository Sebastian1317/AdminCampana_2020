@model AdminCampana_2020.ViewModels.MovilizadoVM


<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="form-group">
            <label>Telefono Celular:</label>
            @Html.TextBoxFor(model => model.Telefono.StrNumeroCelular, null, new { @class = "form-control  input-sm", @readonly = "" })
        </div>
        <div class="form-group">
            <label>Calle:</label>
            @Html.TextBoxFor(model => model.Direccion.StrCalle, null, new { @class = "form-control  input-sm", @readonly = "" })
        </div>
        <div class="form-group">
            <label>Numero Ext:</label>
            @Html.TextBoxFor(model => model.Direccion.StrNumeroExterior, null, new { @class = "form-control  input-sm", @readonly = "" })
        </div>
        <div class="form-group">
            <label>Colonia:</label>
            @Html.TextBoxFor(model => model.Direccion.Colonia.StrAsentamiento, null, new { @class = "form-control  input-sm", @readonly = "" })
        </div>
        <div class="form-group">
            <label>Sección:</label>
            @Html.TextBoxFor(model => model.Direccion.Colonia.Seccion.StrNombre, null, new { @class = "form-control  input-sm", @readonly = "" })
        </div>
        <div class="form-group">
            <label>Zona:</label>
            @Html.TextBoxFor(model => model.Direccion.Colonia.Seccion.Zona.StrNombre, null, new { @class = "form-control  input-sm", @readonly = "" })
        </div>
    </div>
</div>
