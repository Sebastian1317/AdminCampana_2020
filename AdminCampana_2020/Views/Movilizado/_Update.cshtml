@model AdminCampana_2020.ViewModels.MovilizadoVM


@Html.HiddenFor(model => model.Id)
<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="form-group">
            <label>Nombre(s):</label>
            @Html.TextBoxFor(model => model.StrNombre, null, new { @class = "form-control  input-sm" })
        </div>
        <div class="form-group">
            <label>Apellido Paterno:</label>
            @Html.TextBoxFor(model => model.StrApellidoPaterno, null, new { @class = "form-control  input-sm" })
        </div>
        <div class="form-group">
            <label>Apellido Materno:</label>
            @Html.TextBoxFor(model => model.StrApellidoMaterno, null, new { @class = "form-control  input-sm" })
        </div>
        <div class="form-group">
            <label>Telefono Celular:</label>
            @Html.TextBoxFor(model => model.Telefono.StrNumeroCelular, null, new { @class = "form-control  input-sm" })
        </div>
        <div class="form-group">
            <label>Calle:</label>
            @Html.TextBoxFor(model => model.Direccion.StrCalle, null, new { @class = "form-control  input-sm" })
        </div>
        <div class="form-group">
            <label>Numero Ext:</label>
            @Html.TextBoxFor(model => model.Direccion.StrNumeroExterior, null, new { @class = "form-control  input-sm" })
        </div>
        <div class="form-group">
            <label>Colonia:</label>
            @Html.DropDownListFor(model => model.Direccion.idColonia, null, new { @class = "form-control  input-sm" })
        </div>

    </div>
</div>
